<footer>© 시험 데이터 포털 — GitHub Pages에 올리면 그대로 작동합니다. (임베드 차단 시 자동 링크 전환)</footer>

<script>
const tabs=document.querySelectorAll('.tab-btn');
const panels={rate:document.getElementById('tab-rate'),comp:document.getElementById('tab-comp')};
tabs.forEach(btn=>{btn.addEventListener('click',()=>{tabs.forEach(b=>b.classList.remove('active'));btn.classList.add('active');const k=btn.dataset.tab;panels.rate.hidden=k!=='rate';panels.comp.hidden=k!=='comp';btn.setAttribute('aria-selected','true');tabs.forEach(b=>b!==btn&&b.setAttribute('aria-selected','false'));})});
document.querySelectorAll('iframe').forEach(f=>{let t=setTimeout(()=>{const wrap=f.parentElement,fb=wrap.querySelector('.fallback');if(fb){fb.classList.remove('hidden');f.style.display='none';}},5000);f.addEventListener('load',()=>clearTimeout(t));f.addEventListener('error',()=>{const wrap=f.parentElement,fb=wrap.querySelector('.fallback');if(fb){fb.classList.remove('hidden');f.style.display='none';}});});
</script>
